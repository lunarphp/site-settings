{
  "name": "lunarphp/site-settings",
  "type": "project",
  "description": "Add site settings to Lunar.",
  "keywords": ["lunar", "laravel", "ecommerce", "e-commerce", "headless", "store", "shop"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lunar",
      "homepage": "https://lunarphp.io/"
    }
  ],
  "require": {
      "php": "^8.0",
      "lunarphp/lunar": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5.10",
      "mockery/mockery": "^1.4.4",
      "orchestra/testbench": "^6.0|^7.0"
  },
  "autoload": {
      "psr-4": {
        "Lunar\\Settings\\": "src/",
        "Lunar\\Settings\\Database\\Factories\\": "database/factories"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Lunar\\Settings\\Tests\\": "tests/"
      }
  },
  "extra": {
    "lunar": {
        "name": "Site Settings"
    },
    "laravel": {
        "providers": [
            "Lunar\\Settings\\SettingsServiceProvider"
        ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
